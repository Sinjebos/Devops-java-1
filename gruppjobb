            System.out.println("Ni förbereder er för att gå ut på eran dejt kväll");
            System.out.println("Ni gick ut på restaurangen och bion och båda fick corona och dog");
            break;

            case "2":
            System.out.println("Knackandet blir bara mer så du går och öppnar dörren varav din respektive står förbannad över att du inte öppna");
            System.out.println("Ni bestämmer er för att skita i dejt kvällen och ses en annan dag");
            System.out.println("Din respektive hade corona och du blev smittad och dog");
            break;

            default:
            System.out.println("Välj mellan 1 och 2");
            break;

        }

    }
